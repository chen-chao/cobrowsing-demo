{
    "manifest_version": 3,
    "name": "cobrowsing",
    "description": "Extension for cobrowsing communication",
    "version": "1.0",
    "action": {
      "default_popup": "cobrowsing.html",
      "default_icon": "images/favicon.ico"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"]
  }